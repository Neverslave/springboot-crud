<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper  namespace="com.yuedanet.mapper.UserMapperCustom">
  <select id="queryUserSimplyInfoById" resultType="com.yuedanet.pojo.User" parameterType="java.util.List">
    select *
    from user
    where
    id= #{id,jdbcType=VARCHAR}
  </select>
</mapper>